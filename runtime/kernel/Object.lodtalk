"Object methods"
self class: Object.

self category: 'accessing'.
self method [
name
    ^ self printString
].

self method [
yourself
    ^ self
].

self category: 'evaluating'.
self method [
value
    ^ self
].

self method [
valueWithArguments
    ^ self
].

self category: 'converting'.
self method [
asString
    ^ self printString
].

self category: 'printing'.
self method [
printString
    ^ self class name
].
